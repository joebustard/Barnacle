<Defs>
  <!--   Basic Template -->
  <ProjectDefinition Name="Basic">
    <Description>
      Creates a basic folder structure to save the new model.
      The folders are
      /export
      A place to save any STL files exported from the project
      /printer
      A place to save 3D Printer files created by the external slicer.
      /docs
      A place to store documentation such as design notes and build instructions
      /images
      A place to save any image files that will be used to create the model
      /scripts
      A place to save any limpet scripts

      These folders are simply placeholders.
      A single model file called untitled.txt is created in the project root.
    </Description>
    <Folder Name="." Explorer="True" Clean="False" Export="True" AddSubs="True" AddFiles="True">
      <File Name="Untitled.txt" Source="templates/blankmodel1_35.txt" />
    </Folder>
    <Folder Name="Backup" AddSubs="True" AddFiles="True" Explorer="False" Clean="False" Export="False" />
    <Folder Name="Images" Explorer="True" Clean="False" Export="False" AddSubs="True" AddFiles="True" AutoLoad="True"/>

    <Folder Name="Export" Explorer="False" Clean="True" Export="False" AddSubs="False" AddFiles="False" AutoLoad="True" TimeDependency="." />
    <Folder Name="Printer" Explorer="False" Clean="True" Export="True" AddSubs="False" AddFiles="False" AutoLoad="True" TimeDependency="Export" />
    <Folder Name="Scripts" Explorer="True" Clean="False" Export="False" AddSubs="True" AddFiles="True" AutoLoad="True">
           <File Name="CopyToPrint3D.cmd" Source="templates/CopyToPrint3D.cmd" />
           <File Name="OpenAllExportSTLsInCura.cmd" Source="templates/OpenAllExportSTLsInCura.cmd" />
    </Folder>
    <Folder Name="Docs" Explorer="True" Export="False" AddSubs="True" AddFiles="True">
      <File Name="BillOfMaterials.docx" Source="templates/bom.docx" />
    </Folder>
  </ProjectDefinition>

